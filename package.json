{
  "name": "agentis",
  "version": "0.1.0",
  "description": "A powerful framework for building autonomous AI agents with memory, planning, and multi-agent coordination",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tavily-test": "ts-node -r tsconfig-paths/register examples/tavily-test.ts",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "showcase": "ts-node -r tsconfig-paths/register examples/agentis-showcase.ts",
    "demo": "npm run showcase",
    "chat": "ts-node -r tsconfig-paths/register examples/conversational-agent.ts",
    "openai": "ts-node -r tsconfig-paths/register examples/openai-agent.ts",
    "multi-provider": "ts-node -r tsconfig-paths/register examples/multi-provider-swarm.ts",
    "swarm": "ts-node -r tsconfig-paths/register examples/agent-swarm.ts",
    "enhanced-swarm": "ts-node -r tsconfig-paths/register examples/enhanced-multi-provider-swarm.ts",
    "basic-swarm": "ts-node -r tsconfig-paths/register examples/enhanced-swarm-basic.ts",
    "dependency-demo": "ts-node -r tsconfig-paths/register examples/dependency-inference-demo.ts",
    "integrated-demo": "ts-node -r tsconfig-paths/register examples/integrated-showcase.ts",
    "feedback-demo": "ts-node -r tsconfig-paths/register examples/feedback-demo.ts",
    "stock-research": "ts-node -r tsconfig-paths/register examples/stock-research-team.ts",
    "crypto-agent": "ts-node -r tsconfig-paths/register examples/autonomous-crypto-openai-agent-fixed.ts",
    "crypto-twitter": "ts-node -r tsconfig-paths/register examples/crypto-twitter-direct-agent.ts",
    "twitter": "ts-node -r tsconfig-paths/register examples/twitter-direct-bot.ts",
    "twitter-bot": "ts-node -r tsconfig-paths/register examples/twitter-direct-bot.ts",
    "twitter-trends": "ts-node -r tsconfig-paths/register -e \"require('./examples/twitter-direct-bot').getTrends()\"",
    "discord": "ts-node -r tsconfig-paths/register examples/discord-bot-simple.ts",
    "discord-bot": "ts-node -r tsconfig-paths/register examples/discord-bot-simple.ts",
    "discord-researcher": "ts-node -r tsconfig-paths/register examples/discord-researcher-bot.ts",
    "discord-swarm": "ts-node -r tsconfig-paths/register examples/discord-agent-swarm.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "autonomous",
    "anthropic",
    "claude",
    "llm",
    "framework"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.38.0",
    "@pinecone-database/pinecone": "^5.0.2",
    "@tavily/core": "^0.3.1",
    "@types/node": "^22.13.5",
    "agent-twitter-client": "^0.0.18",
    "axios": "^1.8.1",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "openai": "^4.85.4",
    "puppeteer": "^24.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/puppeteer": "^5.4.7",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "tsconfig-paths": "^4.2.0"
  }
}
