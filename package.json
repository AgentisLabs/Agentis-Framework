{
  "name": "agentis",
  "version": "0.1.0",
  "description": "A powerful framework for building autonomous AI agents with memory, planning, and multi-agent coordination",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'"
  },
  "keywords": [
    "ai",
    "agents",
    "autonomous",
    "anthropic",
    "claude",
    "llm",
    "framework"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@types/node": "^22.13.5",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "tsconfig-paths": "^4.2.0"
  }
}
